lista = [110,12,45,23,66]

#modo preferito (pythonico)
for elemento in lista:
    print(elemento)

#modo c-style
for i in range(0,len(lista)):
    print(lista[i],end="-")



#calcolo min e max
max = lista[0]
min = lista[0]
for elemento in lista[1:]:
    if(elemento < min):
        min = elemento
    elif(elemento > max):
            max = elemento

print("l'elemento minore e: ", min)
print("l'elemento maggiore e:", max)
