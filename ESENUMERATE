dizionario = {"w":"avanti", "a":"sinistra", "s":"indietro", "d":"destra", "i":"avanti", "j":"sinistra", "k":"indietro", "l":"destra"}

comando = "avanti"
k = 0
tasti = []

for indice in dizionario:
    if(dizionario[indice] == comando):
        tasti.append(indice)

print(tasti)


l = ["ciao", print, 1, 3.24]

for indice, elemento in enumerate(l):
    print(indice,elemento)
