#es 1
s = "chikakok"

print(s[::2])

#es 2
n = int(input("inserisci un numero: "))

lista = [1] * n
lista[0],lista[-1] = 0, 0

print(lista)

#es3
n = int(input("inserisci un numero: "))

l = []
for k in range(1,n+1):
    l.append(k)

print(l)

#es 3 modo veloce
l2 = [i for i in range(1,n+1)] #list comprehension
print(l2)


#proca list comprehension

quadrati = [i*i for i in range(1,11)]
print(quadrati)
